{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "test-app": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/test-app",
      "sourceRoot": "projects/test-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/test-app",
            "index": "projects/test-app/src/index.html",
            "browser": "projects/test-app/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/test-app/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/test-app/public"
              }
            ],
            "styles": ["projects/test-app/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "test-app:build:production"
            },
            "development": {
              "buildTarget": "test-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/test-app/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/test-app/public"
              }
            ],
            "styles": ["projects/test-app/src/styles.css"],
            "scripts": []
          }
        }
      }
    },
    "dm-context-menu": {
      "projectType": "library",
      "root": "projects/dm-context-menu",
      "sourceRoot": "projects/dm-context-menu/src",
      "prefix": "dm",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/dm-context-menu/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dm-context-menu/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/dm-context-menu/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/dm-context-menu/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "dm-faker-pro": {
      "projectType": "library",
      "root": "projects/dm-faker-pro",
      "sourceRoot": "projects/dm-faker-pro/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/dm-faker-pro/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dm-faker-pro/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/dm-faker-pro/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/dm-faker-pro/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "dm-custom-notifier": {
      "projectType": "library",
      "root": "projects/dm-custom-notifier",
      "sourceRoot": "projects/dm-custom-notifier/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/dm-custom-notifier/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dm-custom-notifier/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/dm-custom-notifier/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/dm-custom-notifier/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "dm-rest-api-helper": {
      "projectType": "library",
      "root": "projects/dm-rest-api-helper",
      "sourceRoot": "projects/dm-rest-api-helper/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/dm-rest-api-helper/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dm-rest-api-helper/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/dm-rest-api-helper/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/dm-rest-api-helper/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
